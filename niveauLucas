elif niveau == 1:
        background.Background(self,"images/backgrounds/background_1.png")
        self.joueur.pos = vec(60,100)

        for plate in [(20,120),(220,0,2),(220,100,2),(220,200,2),(120,300),
        (170,450),(370,320),(478,200,2)]:
